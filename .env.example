# Expo Mobile App Environment Example
# Copy to .env (never commit real secrets)

# Base API URL (no trailing slash)
API_BASE_URL=https://dev.material-api.example.com

# Environment tag (DEV|UAT|PRE|PROD)
APP_ENV=DEV

# Feature flags (comma separated or boolean)
FEATURE_APPROVALS=true
FEATURE_ATTACHMENTS=true
FEATURE_OFFLINE_STAGING=true

# Update channel (expo-updates)
EXPO_UPDATE_CHANNEL=development

# Public analytics key (if any)
ANALYTICS_PUBLIC_KEY=

# Log batching endpoint relative path
LOG_ENDPOINT=/log/events

# Permissions cache TTL minutes
PERMISSIONS_TTL_MINUTES=10

# Performance thresholds (ms)
PERF_P90_COLD_START=3200
PERF_P90_SEARCH=1500
PERF_P90_SCAN_SUBMIT=1100
